<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="ClassicIndicator">
        <attr name="dot_style" format="enum">
            <enum name="oval" value="0" />
            <enum name="rect" value="1" />
            <enum name="line" value="2" />
        </attr>
        <attr name="dot_span" format="dimension" />
        <attr name="dot_width" format="dimension" />
        <attr name="dot_height" format="dimension" />
        <attr name="dot_selected_color" format="color" />
        <attr name="dot_unselected_color" format="color" />
    </declare-styleable>

</resources>